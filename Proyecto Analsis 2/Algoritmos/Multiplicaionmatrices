Multiplicaciondematrices(array[],i,j) {
        if (i = j) then
            return 0
        min <- âˆž
        for (int k <- i; k < j; k++) do
            count <- MatrixChainOrder(p, i, k)+ MatrixChainOrder(p, k + 1, j)+ p[i - 1] * p[k] * p[j]
            if (count < min) then
                min <- count
        return min