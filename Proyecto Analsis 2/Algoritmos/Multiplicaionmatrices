multiplicar(array[],i,j) 
        if (i = j) then
            return 0
        min <- âˆ
        for (int k <- i; k < j; k++) do
            conjunto1 <- multiplicar(array, i, k, idAmbiente)
            conjunto2 <- multiplicar(array, k + 1, j, idAmbiente)
            count <- conjunto1 + conjunto2 + array[i - 1] * array[k] * array[j]
            if (count < min) then
                min <- count
        return min